{
    "name": "unstock-admin",
    "version": "1.0.1",
    "private": true,
    "scripts": {
        "dev": "next dev -p 5000",
        "dev:debug": "NODE_OPTIONS='--inspect' next dev -p 5000",
        "build": "next build",
        "export": "next export",
        "start": "next start",
        "test": "jest __tests__/",
        "test:watch": "jest __tests__/ --watch",
        "test:coverage": "jest __tests__/ --coverage --collectCoverageFrom=domain/**/*.ts --collectCoverageFrom=data/**/*.ts",
        "test:integration": "jest __tests__/integration",
        "test:integration:coverage": "jest __tests__/integration --coverage --collectCoverageFrom=data/**/*.ts ",
        "test:integration:watch": "jest __tests__/integration --watch",
        "test:unit": "jest __tests__/unit",
        "test:unit:coverage": "jest __tests__/unit --coverage --collectCoverageFrom=domain/**/*.ts ",
        "test:unit:watch": "jest __tests__/unit --watch",
        "test:ci": "jest --ci",
        "tslint": "tslint -p . -c tslint.json",
        "tslint:fix": "tslint -p . -c tslint.json --fix",
        "tslint-check": "tslint-config-prettier-check ./tslint.json",
        "prettier": "prettier --write pages domain components config lang public data __tests__"
    },
    "jest": {
        "collectCoverageFrom": [
            "*/{!(ignore-me),}.ts"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn tslint"
        }
    },
    "engines": {
        "node": ">=10.17"
    },
    "dependencies": {
        "@geist-ui/react": "^2.1.0-canary.2",
        "@geist-ui/react-icons": "^1.0.1",
        "@sendgrid/mail": "^7.4.0",
        "@types/react": "^16.9.44",
        "aws-sdk": "^2.750.0",
        "chart.js": "^2.9.4",
        "dotenv": "^8.2.0",
        "ejs": "^3.1.5",
        "formidable": "^1.2.2",
        "image-size": "^0.9.1",
        "jsonwebtoken": "^8.5.1",
        "leaflet": "^1.7.1",
        "mime-types": "^2.1.27",
        "mjml": "^4.7.1",
        "moment": "^2.29.1",
        "mustache": "^4.1.0",
        "next": "^10.0.2",
        "next-auth": "^3.1.0",
        "pg": "^8.3.0",
        "polylabel": "^1.1.0",
        "react": "16.13.1",
        "react-chartjs-2": "^2.11.1",
        "react-dom": "16.13.1",
        "react-dropzone": "^11.0.3",
        "react-leaflet": "^3.0.2",
        "serverless-postgres": "^1.8.0",
        "superagent": "^6.1.0",
        "typescript": "^3.9.7",
        "validator": "^13.1.17"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.11.1",
        "@testing-library/react": "^10.4.7",
        "@types/pg": "^7.14.4",
        "@types/polylabel": "^1.0.5",
        "@types/superagent": "^4.1.10",
        "babel-jest": "^26.1.0",
        "babel-plugin-module-resolver": "^4.0.0",
        "husky": "^4.2.5",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.1.0",
        "prettier": "^2.0.5",
        "react-test-renderer": "^16.13.1",
        "tslint": "^6.1.3",
        "tslint-config-airbnb": "^5.11.2",
        "tslint-config-prettier": "^1.18.0",
        "tslint-react": "^5.0.0",
        "tslint-react-hooks": "^2.2.2"
    }
}
